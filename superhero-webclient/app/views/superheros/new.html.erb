<h1>New Souper Gyro</h1>
<%= form_tag '/superheros', method: :post do %>
  
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name, @superhero["name"] %>
  </div>
  
  <div>
    <%= label_tag :power %>
    <%= text_field_tag :power, @superhero["power"] %>
  </div>

<%= submit_tag "Make a New Superhero!!!" %>
<% end %>